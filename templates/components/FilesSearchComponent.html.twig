{% extends theme.themeDirectory ~ "/partials/_datatable.html.twig" %}

{% block rows %}
	{% for file in pagination %}
		{% include "admin/files/_row.html.twig" with {file: file} %}
	{% else %}
		<tr>
			<td colspan="6" class="px-6 py-3">{{ 'No Result Found'|trans }}</td>
		</tr>
	{% endfor %}
{% endblock %}
